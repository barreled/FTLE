# This script will move the .tif and .avi files out of the ouput folder into whatever folder you specify
cd ../output
rm *.csv
rm *.bin

mkdir -p './videos'
mv *.avi $_

mkdir -p './cannula3zoomed/off'
mv *.tif $_
mv *.mat $_
mv *.info $_


cd $_
mkdir -p ./forward;
mv *forward* $_

mkdir -p ./reverse;
mv *reverse* $_