# This script will move the .tif and .avi files out of the ouput folder into whatever folder you specify
cd ../output
rm *.csv
rm *.bin

mkdir -p './videos'
mv *.avi $_

mkdir -p './Tilting Disc - Septum/11k'
mv *.tif './Tilting Disc - Septum/11k'
mv *.mat './Tilting Disc - Septum/11k'
mv *.info './Tilting Disc - Septum/11k'
mv *.txt './Tilting Disc - Septum/11k'


cd './Tilting Disc - Septum/11k'
mkdir -p ./forward;
mv *forward* $_

mkdir -p ./reverse;
mv *reverse* $_